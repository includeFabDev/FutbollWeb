extends  ../_skeletonblock variables	- pageName = 'Buttons'//- Pageblock page	include ../sections/_header-center	+breadcrumbs-header({		text: "Choose Your Style"	})	='\n'		// Main Button types	section.section.section-lg.bg-default.text-center		.container.container-fullwidth			h3 Main Button types			.custom-wrap-1				.group-md.group-middle.button-group					.button.button-primary.button-nina default size					.button.button-sm.button-primary.button-nina middle size					.button.button-xs.button-primary.button-nina small size					.button.button-dark.button-nina default size					.button.button-sm.button-dark.button-nina middle size					.button.button-xs.button-dark.button-nina small size	="\n"		// Buttons with left Icon	section.section.section-lg.bg-default.text-center		.container.container-fullwidth			h3 Buttons with left Icon			.custom-wrap-1				.group-md.group-middle.button-group					.button.button-icon.button-icon-left.button-primary.button-nina						span.icon.mdi.mdi-magnify						| Icon Left					.button.button-icon.button-icon-left.button-sm.button-primary.button-nina						span.icon.mdi.mdi-magnify						| Icon Left					.button.button-icon.button-icon-left.button-xs.button-primary.button-nina						span.icon.mdi.mdi-magnify						| Icon Left					.button.button-icon.button-icon-left.button-dark.button-nina						span.icon.mdi.mdi-magnify						| Icon Left					.button.button-icon.button-icon-left.button-sm.button-dark.button-nina						span.icon.mdi.mdi-magnify						| Icon Left					.button.button-icon.button-icon-left.button-xs.button-dark.button-nina						span.icon.mdi.mdi-magnify						| Icon Left	="\n"		// Buttons with right Icon	section.section.section-lg.bg-default.text-center		.container.container-fullwidth			h3 Buttons with right Icon			.custom-wrap-1				.group-md.group-middle.button-group					.button.button-icon.button-icon-right.button-primary.button-nina						| Right icon						span.icon.mdi.mdi-arrow-right					.button.button-icon.button-icon-right.button-sm.button-primary.button-nina						| Right icon						span.icon.mdi.mdi-arrow-right					.button.button-icon.button-icon-right.button-xs.button-primary.button-nina						| Right icon						span.icon.mdi.mdi-arrow-right					.button.button-icon.button-icon-right.button-dark.button-nina						| Right icon						span.icon.mdi.mdi-arrow-right					.button.button-icon.button-icon-right.button-sm.button-dark.button-nina						| Right icon						span.icon.mdi.mdi-arrow-right					.button.button-icon.button-icon-right.button-xs.button-dark.button-nina						| Right icon						span.icon.mdi.mdi-arrow-right	="\n"		// Alternative Buttons with left Icon	section.section.section-lg.bg-default.text-center		.container.container-fullwidth			h3 Buttons with shadow			.custom-wrap-1				.group-md.button-group					.button.button-primary.button-nina.button-shadow default size					.button.button-sm.button-primary.button-nina.button-shadow middle size					.button.button-xs.button-primary.button-nina.button-shadow small size					.button.button-dark.button-nina.button-shadow default size					.button.button-sm.button-dark.button-nina.button-shadow middle size					.button.button-xs.button-dark.button-nina.button-shadow small size	="\n"	hr	// Page Footer	include ../sections/_footer